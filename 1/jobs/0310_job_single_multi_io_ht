; multiple threads perform i/o on same CPU core
;
; NOTE: this tests is system topology dependent
; 
; logical CPUs #0 and #80 are on same core (HT enabled)
;
; - start -
[global]
rw=${MODE}
size=${SIZE}
bsrange=1k-128k
filename=${DEV}
cpus_allowed=0,80
end_fsync=1

[job1]
ioengine=libaio
numjobs=4

; - end -
